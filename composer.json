{
    "name": "zzz/zzz",
    "description": "Zzz - Python/PHP/JSON Projekt",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "justinrainbow/json-schema": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zzz\\": "src/php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zzz\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-text"
    }
}
