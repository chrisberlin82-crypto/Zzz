{
  "name": "vente-crm-backend",
  "version": "1.0.0",
  "description": "Vente CRM Backend - Energievertrieb CRM System",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:status": "npx sequelize-cli db:migrate:status",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed",
    "test": "jest --forceExit --detectOpenHandles",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "lint": "eslint src/ --ext .js"
  },
  "dependencies": {
    "@turf/turf": "^7.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.11",
    "response-time": "^2.3.2",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "stripe": "^14.11.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "sequelize": {
    "config": "src/config/database.js",
    "models-path": "src/models",
    "seeders-path": "src/database/seeders",
    "migrations-path": "src/database/migrations"
  }
}
